          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/rest/geocodes/api/v1/countries/{id}": {
      "get": {
        "description": "Get Country",
        "tags": [
          "Country"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the Country",
